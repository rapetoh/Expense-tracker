{
	"cli": {
		"version": ">= 15.0.15",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_PROJECT_GROUP_ID": "expense-tracker",
				"EXPO_PUBLIC_REVENUE_CAT_API_KEY": "appl_YOUR_ACTUAL_KEY_HERE",
				"EXPO_PUBLIC_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_PROXY_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_HOST": "192.168.1.2:4000"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_PROJECT_GROUP_ID": "expense-tracker",
				"EXPO_PUBLIC_REVENUE_CAT_API_KEY": "appl_YOUR_ACTUAL_KEY_HERE",
				"EXPO_PUBLIC_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_PROXY_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_HOST": "192.168.1.2:4000"
			}
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"EXPO_PUBLIC_PROJECT_GROUP_ID": "expense-tracker",
				"EXPO_PUBLIC_REVENUE_CAT_API_KEY": "appl_YOUR_ACTUAL_KEY_HERE",
				"EXPO_PUBLIC_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_PROXY_BASE_URL": "http://192.168.1.2:4000",
				"EXPO_PUBLIC_HOST": "192.168.1.2:4000"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
